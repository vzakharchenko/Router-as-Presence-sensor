{
  "name": "router-ui",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --hot --mode=development",
    "start:80": "NODE_ENV=development webpack-dev-server --hot --mode=development --host 0.0.0.0 --port 80",
    "start:prod_test2": "npm install && npm run build:prod && cd staticServer && npm i && npm run start ",
    "build:dev": "NODE_ENV=development webpack -p --mode=development ",
    "build:prod": "NODE_ENV=production ./node_modules/.bin/webpack -p --mode=production ",
    "lint": "eslint --quiet --ext .js src",
    "lint:fix": "eslint --fix --quiet --ext .js src"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-3": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-mobx": "^2.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.0.0",
    "es2015": "0.0.0",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-native": "^3.5.0",
    "html-webpack-plugin": "^3.2.0",
    "progress-bar-webpack-plugin": "^1.11.0",
    "style-loader": "^0.23.1",
    "uglify-js": "^3.4.9",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "duration": "^0.2.2",
    "mobx": "^5.8.0",
    "mobx-react": "^5.4.3",
    "mobx-utils": "^5.1.0",
    "react": "^16.6.3",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.6.3",
    "react-loader-spinner": "^2.3.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "react",
      "react-native"
    ],
    "extends": "airbnb",
    "rules": {
      "no-undef": 0,
      "react/prop-types": 0,
      "import/extensions": 0,
      "react/destructuring-assignment": 0,
      "import/prefer-default-export": 0,
      "import/no-extraneous-dependencies": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ]
    }
  },
  "babel": {
    "presets": [
      "@babel/react",
      "mobx"
    ]
  }
}
