{
  "name": "router-ui",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --hot --mode=development",
    "start:80": "NODE_ENV=development webpack-dev-server --hot --mode=development --host 0.0.0.0 --port 80",
    "start:prod_test2": "npm install && npm run build:prod && cd staticServer && npm i && npm run start ",
    "build:dev": "NODE_ENV=development webpack -p --mode=development ",
    "build:prod": "NODE_ENV=production ./node_modules/.bin/webpack -p --mode=production ",
    "lint": "eslint --quiet --ext .js src",
    "lint:fix": "eslint --fix --quiet --ext .js src"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-stage-3": "^7.8.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-mobx": "^2.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^4.0.0",
    "es2015": "0.0.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.4",
    "eslint-plugin-react-native": "^3.8.1",
    "html-webpack-plugin": "^4.3.0",
    "progress-bar-webpack-plugin": "^2.1.0",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.8",
    "uglify-js": "^3.10.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "duration": "^0.2.2",
    "mobx": "^5.15.4",
    "mobx-react": "^6.2.3",
    "mobx-utils": "^5.6.1",
    "react": "^16.13.1",
    "react-bootstrap": "^0.33.1",
    "react-dom": "^16.13.1",
    "react-loader-spinner": "^3.1.14"
  },
  "eslintConfig": {
    "root": true,
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "react",
      "react-native"
    ],
    "extends": "airbnb",
    "rules": {
      "no-undef": 0,
      "react/prop-types": 0,
      "import/extensions": 0,
      "react/destructuring-assignment": 0,
      "import/prefer-default-export": 0,
      "import/no-extraneous-dependencies": 0,
      "jsx-a11y/control-has-associated-label": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ]
    }
  },
  "babel": {
    "presets": [
      "@babel/react",
      "mobx"
    ]
  }
}
